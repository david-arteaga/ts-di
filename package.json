{
  "name": "ts-express-starter",
  "version": "1.0.0",
  "description": "A starter template for Express using Typescript",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "watch": "tsc --watch",
    "start-dev": "pm2 start ecosystem.config.js --no-daemon",
    "start": "echo 'WITH IN PRODUCTION ENV' & pm2 start ecosystem.config.js --env production",
    "stop": "pm2 kill",
    "show": "pm2 show ts-express-starter"
  },
  "author": "David Arteaga",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "bookshelf": "^0.13.3",
    "bookshelf-cascade-delete": "^2.0.1",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "inversify": "^4.13.0",
    "knex": "^0.15.2",
    "minimatch": "^3.0.4",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "source-map-support": "^0.5.6",
    "typescript": "^3.0.1",
    "pm2": "^3.0.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.23",
    "@types/body-parser": "^1.17.0",
    "@types/bookshelf": "^0.9.9",
    "@types/debug": "0.0.30",
    "@types/express": "^4.16.0",
    "@types/knex": "0.14.19",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.6",
    "@types/source-map-support": "^0.4.1"
  }
}
